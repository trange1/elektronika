[i486_win95]
	Desc    Windows 95/98
	CDName  i486_95
	DefLP   C:\ptc
	Type    WIN32
[i486_nt]
	Desc    Intel on Windows NT 4.0
	CDName  i486_nt
	DefLP   C:\ptc
	Type    WIN32
[ia64_nt]
	Desc    Intel Itanium
	CDName  ia64_nt
	DefLP   C:\ptc
	Type    WIN32
[x86e_win64]
	Desc    Windows XP64 for Extended Systems
	CDName  x86e_win64
	DefLP   C:\ptc
	Type    WIN32
[sun4_solaris]
	Desc    Sun Microsystems (Solaris 2.6)
	CDName  sun4sol
	DefLP   /opt/ptc
	Type    UNIX
	TarFlag -xBfb
	DdFlag  ibs=126b
	PsCmd   /bin/ps -ef
	RshCmd  rsh
	RcPath  /etc/rc2.d
	PSX_sh  /bin/sh
	LibPath LD_LIBRARY_PATH
